#set($layout="layout/layout.vm")
#set( $page_title = "documents")

<header>
    <div class="navbar-fixed">
        <nav class="index-nav">
            <div class="container">
                <div class="nav-wrapper">
                    <a href="/crainnogao/?pageNum=1">
                        <img src="/images/crainnogao.png" alt="" width=";" height=";">
                    </a>
                    <ul class="right hide-on-med-and-down">
                        <li><a href="/crainnogao/?pageNum=1">首页</a></li>
                        <li><a href="/crainnogao/documents">归档</a></li>
                        <li><a href="/crainnogao/about">关于</a></li>
                    </ul>
                </div>
            </div>
        </nav>
    </div>
</header>
<main id="main">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="card white darken-2">
                    <div class="card-content black-text">
                        <blockquote>归档目录</blockquote>
                        <div class="collection">
                            #foreach($logList in $logsList)
                                <a class="collection-item" href="/crainnogao/detail?logId=$!{logList.logId}">#dateTimeText($!{logList.logCreated},'yyyy-MM-dd') $!{logList.getLogTitle()}</a>
                            #end
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>

<footer>
    <div class="footer-copyright">
        <div class="container">
            <span>Copyright © crainnogao2016</span>
        </div>
    </div>
</footer>