
<div class="tab-container">
    <table class="table table-striped table-hover">
        <thead>
        <tr>
            <th>用户ID</th>
            <th>用户名</th>
            <th>上次登录时间</th>
            <th>用户创建时间</th>
            <th>操作</th>
        </tr>
        </thead>
        <tbody>
        #foreach($user in $list)
        <tr>
            <td>$!{user.uid}</td>
            <td>$!{user.userName}</td>
            <td>$!{user.loginTime}</td>
            <td>$!{user.created}</td>
            <td>操作</td>
        </tr>
        #end

        </tbody>
    </table>
</div>